{
    "name": "techvehicle",
    "version": "1.0.0",
    "description": "techvehicle backend resource serve",
    "main": "index.js",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "deploy": "npm run clean && npm run build && aws lambda update-function-code --function-name techvehicle --zip-file fileb://build.zip --publish",
        "clean": "rm build.zip",
        "build": "zip -r build.zip node_modules index.js public src LICENSE.File README.md",
        "start": "BRANDS_TABLE='brands' CATEGORIES_TABLE='categories' CENTER_TABLE='centers' CHECKLIST_TABLE='checkLists' FUELTYPE_TABLE='fueltypes' MODEL_TABLE='models' PROFILE_TABLE='profiles' SERVICE_TABLE='services' VEHICLE_TABLE='vehicles' node app-local",
        "dynamodb-local-run": "docker-compose up",
        "create-database-brand": "aws dynamodb create-table --cli-input-json file://src/brand.json --endpoint-url http://localhost:8080",
        "create-database-category": "aws dynamodb create-table --cli-input-json file://src/category.json --endpoint-url http://localhost:8080",
        "create-database-centers": "aws dynamodb create-table --cli-input-json file://src/centers.json --endpoint-url http://localhost:8080",
        "create-database-checkList": "aws dynamodb create-table --cli-input-json file://src/checklist.json --endpoint-url http://localhost:8080",
        "create-database-fuelType": "aws dynamodb create-table --cli-input-json file://src/fueltype.json --endpoint-url http://localhost:8080",
        "create-database-model": "aws dynamodb create-table --cli-input-json file://src/model.json --endpoint-url http://localhost:8080",
        "create-database-profile": "aws dynamodb create-table --cli-input-json file://src/profile.json --endpoint-url http://localhost:8080",
        "create-database-vehicle": "aws dynamodb create-table --cli-input-json file://src/service.json --endpoint-url http://localhost:8080",
        "create-database-service": "aws dynamodb create-table --cli-input-json file://src/vehicle.json --endpoint-url http://localhost:8080"
    },
    "keywords": [
        "techvehicle",
        "backend",
        "node.js",
        "dyanamodb",
        "express.js"
    ],
    "author": "Kumindu Induranga Ranawaka",
    "license": "ISC",
    "dependencies": {
        "aws-sdk": "^2.834.0",
        "aws-serverless-express": "^3.4.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "i": "^0.3.6",
        "npm": "^6.14.11",
        "uuid": "^8.3.2"
    }
}
